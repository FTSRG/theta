specification Rooks(n : integer) {
	type File : [0 to n - 1]
	type Rank : [0 to n - 1]
	
	function occupied(f : File, r : Rank) : boolean
	const a : File := 0
	
	constraint occupied(a, 0)
	constraint forall (f : File) : exists (r : Rank) :  occupied(f, r)
	constraint forall (f : File, r1 : Rank, r2: Rank) : occupied(f, r1) and occupied(f, r2) imply r1 = r2
	constraint forall (f1 : File, f2 : File, r: Rank) : occupied(f1, r) and occupied(f2, r) imply f1 = f2
}