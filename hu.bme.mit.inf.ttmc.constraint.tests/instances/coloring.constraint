specification Coloring(n : natural, k : natural) {
	type Vertex : [0 to n]
	type Color : [0 to k]
	
	function adjacent(v1 : Vertex, v2 : Vertex) : boolean
	function color(v : Vertex) : Color
	
	constraint forall (v1 : Vertex, v2 : Vertex) : adjacent(v1, v2) equal adjacent (v2, v1)
	constraint forall (v1 : Vertex, v2 : Vertex) : adjacent(v1, v2) imply color(v1) /= color(v2)
}