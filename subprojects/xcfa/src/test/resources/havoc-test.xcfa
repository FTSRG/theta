main process valami {
    main procedure mainProcedure() {
        var x: int
        init loc L0
        loc L1
        loc L2
        loc L3
        final loc END
        error loc ERR

// currently havoc'd statements are assigned the values zero
        L0 -> L1 { x := 1 }
        L1 -> L2 { havoc x }
        L2 -> END { assume x = 0 }
        L2 -> ERR { assume not (x = 0) }
    }
}
