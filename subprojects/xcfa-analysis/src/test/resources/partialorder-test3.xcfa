main process process1 {

	main procedure mainProcedure(){
		init loc L0
		loc L1
		final loc L2
		var y : int

        L0 -> L1 { // b
            assume x = 1
        }
        L1 -> L2 {
            x := 2
        }
        L0 -> L2 { // d
            y := y
        }
	}
}

process process2 {

	main procedure mainProcedure(){
		init loc L0
		loc L1
		loc L2
		final loc L3
		error loc LE

		var y : int

        L0 -> L1 {
            y := y
        }

        L1 -> L2 {
            x := 1
        }

        L2 -> L3 {
            assume x = 1
        }

        L2 -> LE {
            assume x = 2
        }
	}
}