var x : int
var y : int

main process mainProcess{

	main procedure mainProcedure(){
		init loc L0
		loc L1
		loc L2
		loc L3
		final loc L4

		L0 -> L1 {
			atomic-begin
		}
		L1 -> L2 {
			x := 1
		}
		L2 -> L3 {
			y := 1
		}
		L3 -> L4 {
			atomic-end
		}
	}

}

process process1 {
	main procedure mainProcedure() {
		init loc L0
		final loc L1
		error loc L2

		L0 -> L1 {
			assume x = 0 and y = 0 or x = 1 and y = 1
		}

		L0 -> L2 {
			assume not (x = 0 and y = 0 or x = 1 and y = 1)
		}
	}
}
